<Application x:Class="TowerDefense.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:TowerDefense"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <Image x:Key="Open" Source="pack://application:,,,/Resources/Open.jpg"></Image>
        <Image x:Key="Wall" Source="pack://application:,,,/Resources/Wall.png"></Image>
        <Image x:Key="StartPoint" Source="pack://application:,,,/Resources/StartPoint.png"></Image>
        <Image x:Key="EndPoint" Source="pack://application:,,,/Resources/EndPoint.png"></Image>
    </Application.Resources>
</Application>
